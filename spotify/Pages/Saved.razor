@page "/saved"
@inject NavigationManager NavManager
@inject ISpotify spotify
General grid with its most common features

@if (MyData != null)
{
   
}


@code {
    public IEnumerable<FullTrack> MyData = null;


    SpotifyClient spotifyClient = null;

    protected override async Task OnInitializedAsync()
    {
        var result = await spotify.Init();
        if (!result.Success)
        {
            NavManager.NavigateTo(result.RedirectURL);
        }
        spotifyClient = result.SpotifyObject;

        MyData = (await spotifyClient.Library.GetTracks()).Items.Select(ss => ss.Track);
    }



}




